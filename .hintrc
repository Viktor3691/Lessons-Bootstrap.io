{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "empty-table-header": "off",
        "aria-roles": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust",
          "-webkit-overflow-scrolling",
          "user-select",
          "display: block math"
        ]
      }
    ],
    "css-prefix-order": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  }
}